class Solution {
public:
    bool uniqueOccurrences(vector<int>& arr) {
        
    map<int, int> mp;
    
    for(auto i: arr){
        mp[i]++;
    }
    
    set<int> s;
    for(auto it: mp){
        if(s.find(it.second) != s.end())
            return false;
        else
            s.insert(it.second);
    }
    
    return true;
    
    
}
};
